{
  "name": "expected-value",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"npm:dev:electron\"",
    "dev:electron": "wait-on http://localhost:5173 && tsup --watch --silent --onSuccess=\"cross-env NODE_ENV=development electron ./dist-electron/main.js\"",
    "start:prod": "cross-env NODE_ENV=production electron ./dist-electron/main.js",
    "build": "vite build && tsup",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/electron": "^1.6.12",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.1.2",
    "wait-on": "^8.0.1",
    "cross-env": "^10.0.0",
    "electron": "^38.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
